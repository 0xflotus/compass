    "gulp-webserver": "^0.9.1",
    "jadeify": "^4.3.0",
    "less-plugin-clean-css": "^1.5.0",
    "marked": "^0.3.3",
    "merge-stream": "^0.1.7",
    "mocha": "^2.2.5",
    "nightmare": "^1.8.2",
    "node-notifier": "^4.2.3",
    "phantomjs": "^1.9.17",
    "pre-commit": "^1.0.10",
