{
  "name": "scout",
  "description": "The MongoDB GUI.",
  "version": "0.1.2",
  "main": "main.js",
  "electron": {
    "version": "0.28.3"
  },
  "check": {
    "ignore": [
      "build/**",
      "dist/**"
    ]
  },
  "dependency-check": {
    "entries": [
      "./src/index.js"
    ],
    "ignore": [
      "bootstrap",
      "font-awesome",
      "octicons",
      "app",
      "auto-updater",
      "crash-reporter",
      "browser-window",
      "menu"
    ]
  },
  "fonts": [
    "node_modules/font-awesome/fonts/fontawesome-webfont*",
    "node_modules/octicons/octicons/octicons.{eot,svg,ttf,woff}",
    "fonts/*"
  ],
  "less": {
    "paths": [
      "node_modules/font-awesome/less",
      "node_modules/bootstrap/less",
      "node_modules/octicons/octicons"
    ]
  },
  "scripts": {
    "start": "gulp",
    "develop": "gulp develop",
    "test": "mocha",
    "browser-test": "zuul --local 3001 --open -- test/*.test.js",
    "check": "mongodb-js-precommit"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout.git"
  },
  "dependencies": {
    "ampersand-app": "^1.0.4",
    "ampersand-collection": "^1.4.5",
    "ampersand-collection-lodash-mixin": "^2.0.1",
    "ampersand-collection-rest-mixin": "^4.2.0",
    "ampersand-model": "^5.0.3",
    "ampersand-router": "^3.0.2",
    "ampersand-state": "^4.5.6",
    "ampersand-sync-localforage": "^0.1.1",
    "ampersand-view": "^8.0.0",
    "ampersand-view-switcher": "^2.0.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "d3": "^3.5.5",
    "d3-tip": "^0.6.7",
    "debug": "^2.2.0",
    "domready": "^1.0.8",
    "event-stream": "^3.3.1",
    "fix-path": "^1.1.0",
    "font-awesome": "https://github.com/FortAwesome/Font-Awesome/archive/v4.3.0.tar.gz",
    "jquery": "^2.1.4",
    "local-links": "^1.4.0",
    "lodash": "^3.9.3",
    "moment": "^2.10.3",
    "mongodb-extended-json": "^1.3.1",
    "mongodb-schema": "^2.2.1",
    "numeral": "^1.5.3",
    "octicons": "https://github.com/github/octicons/archive/v2.2.0.tar.gz",
    "phantomjs-polyfill": "0.0.1",
    "qs": "^3.1.0",
    "scout-brain": "http://bin.mongodb.com/js/scout-brain/v0.0.1/scout-brain-0.0.1.tgz",
    "scout-client": "http://bin.mongodb.com/js/scout-client/v0.0.1/scout-client-0.0.1.tgz",
    "scout-server": "http://bin.mongodb.com/js/scout-server/v0.0.1/scout-server-0.0.1.tgz"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "clui": "^0.3.1",
    "del": "^1.2.0",
    "electron-builder": "^2.0.0",
    "eslint-config-mongodb-js": "^0.1.4",
    "gulp": "^3.9.0",
    "gulp-jade": "^1.0.1",
    "gulp-less": "^3.0.3",
    "gulp-shell": "^0.4.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-webserver": "^0.9.1",
    "jadeify": "^4.3.0",
    "less-plugin-clean-css": "^1.5.0",
    "merge-stream": "^0.1.7",
    "mocha": "^2.2.5",
    "mongodb-js-precommit": "^0.1.2",
    "nightmare": "^1.8.2",
    "node-notifier": "^4.2.3",
    "phantomjs": "^1.9.17",
    "pre-commit": "^1.0.10",
    "pretty-hrtime": "^1.0.0",
    "stream-combiner2": "^1.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.3",
    "zuul": "^3.1.0"
  }
}
